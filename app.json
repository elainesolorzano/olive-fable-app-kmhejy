{
  "expo": {
    "name": "Olive & Fable",
    "slug": "Olive & Fable",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-139b9ca7-cb0b-4c8f-a83e-c08db754caa0.png.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-139b9ca7-cb0b-4c8f-a83e-c08db754caa0.png.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a3a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oliveandfable.studio",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:oliveandfable.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-139b9ca7-cb0b-4c8f-a83e-c08db754caa0.png.png",
        "backgroundColor": "#1a3a2e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.oliveandfable.studio",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "oliveandfable",
              "host": "*"
            },
            {
              "scheme": "https",
              "host": "oliveandfable.com",
              "pathPrefix": "/appconfirmed"
            },
            {
              "scheme": "https",
              "host": "oliveandfable.com",
              "pathPrefix": "/reset-password"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/cf4cfcd5-5040-4829-aaa7-a1b46ac67fda.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    },
    "scheme": "oliveandfable"
  }
}